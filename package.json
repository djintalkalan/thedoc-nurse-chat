{
  "name": "the-doc-nurse",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "ios": "npx react-native run-ios",
    "start": "npx react-native start",
    "test": "jest",
    "lint": "eslint .",
    "clean-install": "rm -rf node_modules; rm -rf yarn.lock; rm -rf ios/Pods; rm -rf ios/Podfile.lock; yarn",
    "backup": "git archive --format zip --output ./backup.zip master",
    "clean:cache": "rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && npm cache clean --force && yarn cache clean",
    "pod": "npx pod-install",
    "pod-clean": "cd ios && rm -rf Podfile.lock && rm -rf Pods && pod install --repo-update && cd ..",
    "reset": "npx react-native start --reset-cache",
    "release": "cd android && ./gradlew assembleRelease && cd .. && run-s -c open-diawi open-android-output-folder",
    "bundle": "cd android && ./gradlew bundleRelease && cd .. && run-s -c open-play-console open-android-output-folder",
    "clean": "cd android && ./gradlew clean && cd ..",
    "env-p": "yarn rnuc production.yaml",
    "env-d": "yarn rnuc dev.yaml",
    "env-b": "yarn rnuc beta.yaml",
    "apk-p": "run-s -c kill-all env-p clean release",
    "apk-d": "run-s -c kill-all env-d clean release",
    "apk-b": "run-s -c kill-all env-b clean release",
    "bundle-d": "run-s -c kill-all env-d clean bundle",
    "bundle-p": "run-s -c kill-all env-p clean bundle",
    "bundle-b": "run-s -c kill-all env-b clean bundle",
    "tcp": "adb reverse tcp:9090 tcp:9090",
    "fix-sdk": "(cd ~/Library/Android/sdk/build-tools/33.0.0 && mv d8 dx && cd lib && mv d8.jar dx.jar)",
    "postinstall": "(node ./scripts/fix-carousel.js||true)  && (node ./scripts/adv-mmkv.js||true) && (node ./scripts/fix-react-native.js || true) && (node ./scripts/fix-ultimate-config.js || true) && (node ./scripts/fix-compile.js || true) && run-s env-d pod",
    "kill-all": "((pkill -f '.*GradleDaemon.*' || ./android/gradlew gradle --stop) && (rm -rf ~/.gradle/daemon || true))",
    "dl": "/usr/bin/xcrun simctl openurl booted ",
    "push-a-d": "yarn env-d && appcenter codepush release-react -a thedocshine/nurse-chat-android -d dev -m --target-binary-version",
    "push-a-p": "yarn env-p && appcenter codepush release-react -a thedocshine/nurse-chat-android -d production -m --target-binary-version",
    "push-i-d": "yarn env-d && appcenter codepush release-react -a thedocshine/nurse-chat-ios -d dev -m --target-binary-version",
    "push-i-p": "yarn env-p && appcenter codepush release-react -a thedocshine/nurse-chat-ios -d production  -m --target-binary-version",
    "ipa-d": "run-s env-d clean-ios archive",
    "ipa-b": "run-s env-b clean-ios archive",
    "ipa-p": "run-s env-p clean-ios archive",
    "diawi-d": "run-s env-d clean-ios archive-diawi",
    "diawi-b": "run-s env-b clean-ios archive-diawi",
    "diawi-p": "run-s env-p clean-ios archive-diawi",
    "archive": "run-s generate-archive generate-ipa generate-ipa-diawi",
    "archive-diawi": "run-s generate-archive generate-ipa-diawi",
    "open-ios-output-folder": "open ./ios/output",
    "open-android-output-folder": "open ./android/output",
    "open-diawi": "open -a 'Google Chrome' 'https://www.diawi.com/' --args '--new-window'",
    "open-play-console": "open -a 'Google Chrome' 'https://play.google.com/console/u/0/' --args '--new-window'",
    "open-transporter": "(pkill -x Transporter ||true) && open -n -F -a Transporter --args $PWD'/ios/output/theDocChat.ipa'",
    "clean-ios": "xcodebuild clean -workspace ios/theDocChat.xcworkspace -scheme theDocChat && xcodebuild clean -project ios/theDocChat.xcodeproj -scheme theDocChat && rm -rf ios/build && rm -rf ios/output",
    "generate-archive": "xcodebuild -sdk iphoneos -workspace ios/theDocChat.xcworkspace -scheme theDocChat -configuration Release -archivePath ios/build/theDocChat.xcarchive archive",
    "generate-ipa": "xcodebuild -exportArchive -archivePath ios/build/theDocChat.xcarchive -exportOptionsPlist ios/theDocChat/exportOptions.plist -exportPath ios/output -allowProvisioningUpdates && yarn open-transporter",
    "generate-ipa-diawi": "xcodebuild -exportArchive -archivePath ios/build/theDocChat.xcarchive -exportOptionsPlist ios/theDocChat/exportOptionsDiawi.plist -exportPath ios/output/diawi -allowProvisioningUpdates && yarn open-ios-output-folder"
  },
